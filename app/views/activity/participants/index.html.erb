<p id="notice"><%= notice %></p>

<h1>Activity Participants</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_participants.each do |activity_participant| %>
      <tr>
        <td><%= activity_participant.participation_type %></td>
        <td><%= activity_participant.contribution %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity Participant', new_activity_participant_path %>
