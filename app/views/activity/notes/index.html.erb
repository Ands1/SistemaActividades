<p id="notice"><%= notice %></p>

<h1>Activity Notes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_notes.each do |activity_note| %>
      <tr>
        <td><%= activity_note.comment %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity Note', new_activity_note_path %>
